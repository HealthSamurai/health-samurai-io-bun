{
  "name": "health-samurai",
  "module": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun scripts/dev.ts",
    "start": "bun run css:build && bun scripts/start.ts",
    "css:build": "bunx @tailwindcss/cli -i src/styles/tailwind.css -o public/styles/main.css --minify",
    "css:watch": "bunx @tailwindcss/cli -i src/styles/tailwind.css -o public/styles/main.css --watch",
    "typecheck": "bun scripts/typecheck.ts",
    "routes": "bun scripts/routes.ts",
    "check-pages": "bun scripts/check-pages.ts",
    "migrate": "bun scripts/migrate.ts",
    "migrate:up": "bun scripts/migrate.ts up",
    "migrate:down": "bun scripts/migrate.ts down",
    "migrate:status": "bun scripts/migrate.ts status",
    "migrate:create": "bun scripts/migrate.ts create"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@types/bun": "latest",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "marked": "^17.0.1",
    "parse5": "^8.0.0",
    "shiki": "^3.22.0"
  }
}
