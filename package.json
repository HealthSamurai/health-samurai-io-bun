{
  "name": "health-samurai",
  "module": "src/server.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun scripts/dev.ts start",
    "dev:attach": "bun scripts/dev.ts attach",
    "dev:fg": "bun scripts/dev.ts fg",
    "dev:reload": "bun scripts/dev.ts reload",
    "dev:stop": "bun scripts/dev.ts stop",
    "dev:status": "bun scripts/dev.ts status",
    "dev:logs": "bun scripts/dev.ts logs",
    "start": "bun run css:build && bun scripts/start.ts",
    "css:build": "tailwindcss -i src/styles/tailwind.css -o public/styles/main.css --minify",
    "css:watch": "tailwindcss -i src/styles/tailwind.css -o public/styles/main.css --watch=always",
    "typecheck": "bun scripts/typecheck.ts",
    "routes": "bun scripts/routes.ts",
    "check-pages": "bun scripts/check-pages.ts",
    "migrate": "bun scripts/migrate.ts",
    "migrate:up": "bun scripts/migrate.ts up",
    "migrate:down": "bun scripts/migrate.ts down",
    "migrate:status": "bun scripts/migrate.ts status",
    "migrate:create": "bun scripts/migrate.ts create",
    "sql": "bun scripts/sql.ts",
    "kill": "bun scripts/kill.ts"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@types/bun": "latest",
    "@types/katex": "^0.16.8",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "beautiful-mermaid": "^0.1.3",
    "katex": "^0.16.28",
    "marked": "^17.0.1",
    "parse5": "^8.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "shiki": "^3.22.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.1.0",
    "yaml": "^2.8.2"
  }
}
